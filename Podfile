# platform :ios, '9.0'

target 'Maps' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for Maps
  pod 'GoogleMaps', '4.1.0'
  pod 'GooglePlaces', '4.1.0'
  pod 'Alamofire', '5.2'
  pod 'SwiftyJSON', '4.0'
  pod 'Polyline', '~> 5.0'
end
